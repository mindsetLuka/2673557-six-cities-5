{
  "name": "six-cities",
  "version": "4.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "build": "npm run clean && npm run compile",
    "start-dev": "npm run ",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "generate": "npm run build && node ./dist/main.cli.js --generate 10 ./mocks/generated-offers.tsv http://localhost:3123/api",
    "import:db": "node ./dist/main.cli.js --import ./mocks/generated-offers.tsv admin admin 127.0.0.1 six-cities secret_salt",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/convict": "6.1.6",
    "@types/convict-format-with-validator": "6.0.5",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "pino-pretty": "13.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@typegoose/typegoose": "^11.8.1",
    "@types/express": "^5.0.6",
    "chalk": "5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dayjs": "1.11.18",
    "dotenv": "17.2.3",
    "express-async-handler": "^1.2.0",
    "got": "12.6.1",
    "http-status-codes": "^2.3.0",
    "inversify": "7.10.2",
    "jose": "^5.10.0",
    "json-server": "1.0.0-beta.3",
    "mkdirp": "3.0.1",
    "mongoose": "^7.8.7",
    "pino": "10.0.0",
    "reflect-metadata": "^0.2.2"
  }
}
